<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Instrument+Serif&display=swap" rel="stylesheet">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Bruno+Ace+SC&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="/css/styles.css">

</head>

<body>
  <%-include('../partials/header.ejs')%>
    <section id="banner">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <h4>
              <%=bannerRecord.title%>
            </h4>
            <p>
              <%=bannerRecord.desc%>
            </p>
            <a href="/banner"> <button class="btn btn-success">More Details</button></a>

          </div>
          <div class="col-md-4">
            <img src="/upload/<%=bannerRecord.img%>" class="img-fluid d-block ms-auto" alt="">
          </div>
        </div>
      </div>
    </section>

    <section id="service">
      <div class="container">
        <div class="row ">
          <h3>service</h3>

          <%serviceRecord.forEach((result)=>{%>
            <div class="col-md-4">
              <img src="./upload/<%=result.img%>" class="d-block" alt="" width="100px">
              <h4>
                <%=result.title%>
              </h4>
              <p>
                <%=result.desc%>
              </p>
              <a href="/servicedetails/<%=result.id%>"><button class="btn btn-info">more details...</button></a>
            </div>
            <%})%>
        </div>
      </div>
      </div>
    </section>

    <section id="testi">
      <div class="container">
        <div class="row">
          <h3>testinominals</h3>
          <%testiRecord.forEach((result)=>{%>
            <div class="col-md-4">
              <div class="card" style="width: 18rem;">
                <!-- <i class="bi bi-buildings mx-auto"></i> -->
                <img src="/upload/<%=result.img%>" alt="">
                <div class="card-body">
                  <p class="card-test"> <span><i class="bi bi-quote"></i></span> <%=result.quotes%> <span> <i
                        class="bi bi-quote"></i> </span>
                  </p>
                  <h5 class="card-title">IBM India PVT LTD</h5>
                  <button class="mx-auto d-block btn btn-success form-control"><i class="bi bi-star"></i><i
                      class="bi bi-star"></i><i class="bi bi-star"></i><i class="bi bi-star"></i><i
                      class="bi bi-star"></i></button>
                </div>
              </div>
            </div>
          <%})%>
          
      </div>
      </div>
    </section>


    <section id="profile">
      <div class="container">
        <div class="row">
          <h2>profile</h2>
          <div class="col-md-12 d-flex">
            <div class="card" style="width: 10rem;">
              <img src="/media/business.png" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">Lokesh rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.5.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">devendar rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.4.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">sanjay rao</h5>
                <p class="card-text">app engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.3.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title"> mohit rao</h5>
                <p class="card-text">web engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.2.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">abhi rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
            <div class="card" style="width: 10rem;">
              <img src="./media/business.4.jpg" class="card-img-top" alt="...">
              <div class="card-body">
                <h5 class="card-title">anil rao</h5>
                <p class="card-text">software engineer</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <%-include('../partials/footer.ejs')%>

      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
</body>

</html>